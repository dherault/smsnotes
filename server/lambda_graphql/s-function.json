{
  "name": "lambda_graphql",
  "runtime": "babel",
  "description": "Serverless Lambda function for project: smsnotes",
  "customName": false,
  "customRole": false,
  "handler": "lambda_graphql/handler.default",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": [],
    "runtime": {
      "babel": {
        "presets": ["es2015", "stage-2"]
      },
      "minify": false,
      "requires": ["../../../shared"]
    }
  },
  "endpoints": [
    {
      "path": "graphql",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": {
          "payload": "$input.json('$')",
          "sourceIp": "$context.identity.sourceIp"
        }
      },
      "responses": {
        "400": {
          "statusCode": "400",
          "selectionPattern": ".*\"statusCode\":400.*",
          "responseTemplates": {
            "application/json": "$${errorMessageExtractionTemplate}"
          }
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      }
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "smsnotes",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
