{
  "name": "lambda_sms",
  "runtime": "babel",
  "description": "Serverless Lambda function for project: smsnotes",
  "customName": false,
  "customRole": false,
  "handler": "lambda_sms/handler.default",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "sms",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": {
          "sourceIp": "$context.identity.sourceIp",
          "querystring": "$input.params().querystring",
          "message": {
            "id": "$input.params('messageId')",
            "sender": "$input.params('msisdn')",
            "receiver": "$input.params('to')",
            "content": "$input.params('text')",
            "timestamp": "$input.params('message-timestamp')",
            "type": "$input.params('type')"
          }
        }
      },
      "responses": {
        "400": "$${400ResponseTemplate}",
        "409": "$${409ResponseTemplate}",
        "500": "$${500ResponseTemplate}",
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      }
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "smsnotes",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
