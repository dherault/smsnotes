{
  "name": "smsnotes",
  "version": "0.0.1",
  "description": "Take notes only with sms",
  "main": "index.js",
  "scripts": {
    "start": "npm run offline & npm run dev",
    "offline": "sls offline start -p dev",
    "offline_debug": "sls offline start -p dev --debugOffline",
    "dev": "node webpack/dev_server.js",
    "build": "npm run build:clean && npm run build:js && npm run build:size",
    "build:clean": "rm -f www/bundle.js",
    "build:js": "webpack --config webpack/webpack.config.prod.js --progress",
    "build:size": "du -h www/bundle.js",
    "build:test": "cd www && python -m SimpleHTTPServer 8080",
    "deploy:s3": "aws s3 cp --recursive www/ s3://www.smsnotes.com/ --acl public-read",
    "deploy:cloudfront": "aws cloudfront create-invalidation --distribution-id E22H4NZXOVN1U --paths /*",
    "sms:send": "node scripts/sms/sendOfflineSms.js",
    "sms:flood": "node scripts/sms/floodOfflineSms.js",
    "db:createTables": "babel-node scripts/dynamodb/createTables.js",
    "db:deleteTables": "babel-node scripts/dynamodb/deleteTables.js",
    "db:listTables": "babel-node scripts/dynamodb/listTables.js",
    "db:resetTables": "npm run db:deleteTables && npm run db:createTables && npm run db:listTables"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dherault/smsnotes.git"
  },
  "keywords": [
    "smsnotes",
    "sms",
    "notes",
    "lambda",
    "serverless"
  ],
  "author": "David HÃ©rault <dherault@gmail.com> (https://github.com/dherault)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dherault/smsnotes/issues"
  },
  "homepage": "https://github.com/dherault/smsnotes",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.5.0",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.3",
    "express": "^4.13.4",
    "imports-loader": "^0.6.5",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.2",
    "precss": "^1.4.0",
    "serverless-runtime-babel": "0.0.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
